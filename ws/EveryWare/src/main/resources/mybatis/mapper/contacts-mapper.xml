<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="contactsMapper">

	<select id="selectList" resultType="ContactsVo">
		SELECT 
		   	CON_NO
			, CON_NAME
			, CON_EMAIL
			, CON_TEL
			, CON_COMPANY
		FROM CONTACTS
	</select>








</mapper>