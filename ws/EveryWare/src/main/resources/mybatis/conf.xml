<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >

<configuration>

	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	
	<typeAliases>
		<!-- 전자결재 관련 -->
		<typeAlias alias="DocFormVo" type="com.kh.app00.approval.doc.vo.DocFormVo"/>
		<typeAlias alias="DocPeriodVo" type="com.kh.app00.approval.doc.vo.DocPeriodVo"/>
		<typeAlias alias="DocSecurityVo" type="com.kh.app00.approval.doc.vo.DocSecurityVo"/>
		<typeAlias alias="DocFormMapperVo" type="com.kh.app00.approval.doc.vo.DocFormMapperVo"/>
		<typeAlias alias="DocDataVo" type="com.kh.app00.approval.doc.vo.DocDataVo"/>
