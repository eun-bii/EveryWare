		<typeAlias alias="ApprovalTypeVo" type="com.kh.app00.approval.vo.ApprovalTypeVo"/>
		
		
		<!-- 인사 관련 -->
		<typeAlias alias="EmpVo" type="com.kh.app00.emp.vo.EmpVo"/>
		<typeAlias alias="DeptVo" type="com.kh.app00.organization.vo.DeptVo"/>
		
		
		<!-- 근태 관련 -->
		<typeAlias alias="DayoffVo" type="com.kh.app00.dayoff.vo.DayoffVo"/>
		
		
